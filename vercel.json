{
    "version": 2,
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
        
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/app.js"
      }
    ],
    "functions": {
    "index.js": {
      "maxDuration": 60
    },
    "*.js": {
      "maxDuration": 60
    }
  }
}